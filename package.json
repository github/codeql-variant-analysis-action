{
  "name": "codeql-query",
  "version": "1.0.7",
  "private": true,
  "description": "CodeQL query action",
  "scripts": {
    "build": "node build.mjs",
    "watch": "node build.mjs --watch",
    "test": "ava src/** --serial --verbose --timeout=1m",
    "test-debug": "ava src/** --serial --verbose --timeout=20m",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 . --ext .js,.ts",
    "lint-fix": "eslint --report-unused-disable-directives --max-warnings=0 . --ext .js,.ts --fix",
    "find-deadcode": "ts-node script/find-deadcode.ts"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": "tsc"
    },
    "workerThreads": false
  },
  "license": "MIT",
  "engines": {
    "node": "^20.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/http-client": "^2.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/action": "^6.0.7",
    "@octokit/plugin-retry": "^6.0.1",
    "ajv": "8.12.0",
    "form-data": "^4.0.0",
    "jszip": "3.10.1",
    "yaml": "^2.3.4"
  },
  "//": [
    "micromatch is an unspecified dependency of ava"
  ],
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@octokit/types": "^12.4.0",
    "@types/archiver": "^6.0.2",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "archiver": "^6.0.1",
    "ava": "^6.1.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-async-foreach": "^0.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "micromatch": "^4.0.5",
    "prettier": "^3.2.5",
    "ts-json-schema-generator": "1.5.0",
    "ts-node": "^10.9.2",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "normalize-url": ">=4.5.1"
  }
}